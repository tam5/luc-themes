(require 'luc-themes)

(define-luc-theme oceanic "Oceanic Theme."
  (
    (background-color     "#1B2B34")
    (far-background-color "#1B2B34")
    (foreground-color     "#D8DEE9")
    (highlight-line-color "#2C3B45")
    (selection-color      "#C594C5")
