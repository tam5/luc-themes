    (subtle-color         "#343D46")
    (supporting-1-color   "#A7ADBA")
    (supporting-2-color   "#C0C5CE")

    (comment-color        "#4F5B66")
    (primitive-color      "#AB7967")
    (constant-color       "#EC5f67")
